{
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "sourceMap": false,
        "removeComments": true,
        "downlevelIteration": true,
        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "experimentalDecorators": true
    }
}